{
  "name": "@pomodoro/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "mysql:restore": "mysql -h 127.0.0.1 -P 4000 -u root pomodoro_tests < dumps/dump.sql",
    "mysql:dump": "mysqldump -h 127.0.0.1 -P 4000 --no-create-info --compact -u root pomodoro_tests > dumps/dump.sql",
    "mysql:reset": " npx prisma db push --force-reset && npm run mysql:restore",
    "mysql:connect": "mysql -h 127.0.0.1 -P 4000 -u root pomodoro_tests",
    "prisma:dev-seed": "npx prisma db push --force-reset && npx prisma db seed",
    "int-test": "npx prisma db push --force-reset && npm run mysql:restore && npx nx test --silent=true pomodoro-backend",
    "unit-test:backend": "npx jest --group=unit/backend",
    "unit-test:frontend": "npx jest --group=unit/frontend",
    "compile-less": "npx lessc ./apps/pomodoro-frontend/src/styles.less ./apps/pomodoro-frontend/src/styles.css"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "private": true,
